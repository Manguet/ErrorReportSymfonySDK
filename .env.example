# Error Explorer Configuration
# Copy these variables to your .env file and configure them

# Required: The base URL of your Error Explorer instance
ERROR_WEBHOOK_URL=https://your-error-explorer-domain.com

# Required: Unique project token from your Error Explorer project settings
ERROR_WEBHOOK_TOKEN=your-unique-project-token

# Required: Name identifier for your project
PROJECT_NAME=your-project-name

# Optional: Enable/disable error reporting (true/false)
ERROR_REPORTING_ENABLED=true